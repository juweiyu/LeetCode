def left_searchRange(nums, target, left):
    lo, hi = 0, len(nums)
    while lo < hi:
        mid = (lo + hi) // 2
        if nums[mid] > target or (left and nums[mid] == target):
            hi = mid
        else:
            lo = mid + 1
    return lo


def searchRange(nums, target):
    left = left_searchRange(nums, target, True)
    right = left_searchRange(nums, target, False)
    if left == len(nums) or (nums[left] != target):
        return [-1, -1]
    else:
        return [left, right - 1]


nums = [5, 7, 7, 8, 8, 10]
target = 18
print(searchRange(nums, target))
