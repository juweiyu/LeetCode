# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def preorderTraversal(self, root: TreeNode) -> List[int]:
        output = []
        stack = []
        now = root
        while now or stack:
            while now:
                output.append(now.val)
                stack.append(now)
                now = now.left
            now = stack.pop()
            now = now.right
        return output
            
        
