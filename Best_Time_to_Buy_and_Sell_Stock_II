class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        if not prices:
            return 0
        
        global_gain = 0
        
        for i in range(1, len(prices)):
            delta = prices[i] - prices[i-1]
            if delta >= 0:
                global_gain = global_gain + delta
        
        return global_gain
